{
  "name": "Biazza",
  "version": "1.0",
  "description": "Better Piazza",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "favicon.png"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://piazza.com/class/*"
      ],
      "run_at": "document_idle",
      "css": [
        "contentScript.css"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "permissions": []
}